{
  "name": "Lucky",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "rm -rf dist && mkdir -p dist && bun build src/index.ts --compile --minify --target=bun-linux-x64 --outfile=dist/Lucky",
    "fmt": "biome format --write"
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.3.1",
    "elysia": "latest",
    "json-accelerator": "^0.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "bun-types": "latest"
  },
  "module": "src/index.js"
}
